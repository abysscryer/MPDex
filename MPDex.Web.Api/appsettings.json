{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=(LocalDB)\\MSSQLLocalDB;Database=LogDB;Trusted_Connection=True;MultipleActiveResultSets=true",
          "tableName": "ApiLog",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=MPDexTestDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "redis": {
    "host": "127.0.0.1",
    "port": 6379,
    "name": "localhost"
  }
}
